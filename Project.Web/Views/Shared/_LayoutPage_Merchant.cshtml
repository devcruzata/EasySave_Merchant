    <script type="text/javascript">
        $(document).ready(function () {
            $('input[type="text"]').bind('keypress', function (event) {
                var regex = /[`~!#$%^&*()_|+\-=?;:'",<>\{\}\[\]\\\/]/gi;
                var key = String.fromCharCode(!event.charCode ? event.which : event.charCode);
                if (regex.test(key)) {
                    event.preventDefault();
                    return false;
                }
            });
        });
    </script>


